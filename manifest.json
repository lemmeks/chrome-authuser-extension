{
  "name": "AuthUser Appender",
  "version": "1.0",
  "description": "Automatically append the authuser param to GCP URLs",
  "permissions": [
    "webRequest",
    "*://*.example.com/"
  ],
  "background": {
     "page":"bg.html"
     },
  "content_scripts": [
   {
     "matches": ["http://*/*", "https://*/*"],
     "js": ["content.js"]
   }],
  "manifest_version": 2
 }
